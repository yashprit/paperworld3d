<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>

	<bean id="multiplayer.service" class="com.paperworld.java.adaptor.iphone.IPhoneService" init-method="init">
		<property name="application" ref="web.handler" />
	</bean>
	
	<bean id="udp.service" class="org.red5.server.net.udp.UDPService">
		<property name="multiplayerService" ref="multiplayer.service"/>
	</bean>

</beans>